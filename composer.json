{
    "$schema": "https://getcomposer.org/schema.json",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "description": "The skeleton application for the Laravel framework.",
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "name": "laravel/laravel",
    "prefer-stable": true,
    "require": {
        "php": "^8.3.23",
        "laravel/framework": "^v12.39.0",
        "laravel/scout": "^v10.22.0",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.7",
        "teamtnt/laravel-scout-tntsearch-driver": "^15.0"
    },
    "require-dev": {
        "fakerphp/faker": "^v1.24.1",
        "laravel/breeze": "^v2.3.8",
        "laravel/pail": "*",
        "laravel/pint": "^v1.25.1",
        "laravel/sail": "^v1.48.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^v8.8.3",
        "phpunit/phpunit": "^11.5.44"
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "type": "project"
}
